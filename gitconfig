[core]
	excludesfile = ~/.gitignore_global
[user]
	name = John Flavin
	email = <flavinj@gmail.com>
[push]
	default = simple
[alias]
    st = status
    co = checkout
    com = commit
    br = branch
    hist = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
    l = log --branches --remotes --tags --graph --oneline --decorate
